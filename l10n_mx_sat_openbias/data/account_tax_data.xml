        <!-- IVA VENTAS 

        code		Nombre							Tasa 	 Comentarios
        209.01.01	IVA trasladado no cobrado				16%	 Este se registra cuando se realiza la facturación al cliente y será traspasado al cobrado cuando exista la gestión del pago del cliente
        208.01.01	IVA trasladado cobrado					16%	 Se traspasa del no cobrado cuando el cliente paga
        -->

        <record id="tax0" model="account.tax.template">
            <field name="chart_template_id" ref="mx_coa_sat"/>
            <field name="name">IVA(0%) VENTAS</field>
            <field name="description">IVA(0%) VENTAS</field>
            <field name="amount">0</field>
            <field name="amount_type">percent</field>
            <field name="type_tax_use">sale</field>
            <field name="account_id" ref="l10n_mx_sat_openbias.account_209_01_01"/>
            <field name="refund_account_id" ref="l10n_mx_sat_openbias.account_209_01_01"/>
            <field name="tag_ids" eval="[(6,0,[ref('tax_tag_0')])]"/>
            <field name="tax_group_id" ref="cfd_mx.tax_group_iva"/>
        </record>
        <record id="tax1" model="account.tax.template">
            <field name="chart_template_id" ref="mx_coa_sat"/>
