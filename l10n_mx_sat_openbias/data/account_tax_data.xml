        <!-- IVA COMPRAS

        code		Nombre							Tasa 	 Comentarios
        119.01.01	IVA pendiente de pago					16%	 Cuando se registra el compromiso de pago (provisión de pasivo), se registra el IVA acreditable no pagado y se hace su traspaso al IVA Acreditable pagado cuando exista flujo de efectivo 
        118.01.01	IVA acreditable pagado					16%	 Se registra el traspaso del IVA acreditable no pagado al pago (cuando se pague la erogación)

        119.02.01	IVA de importación pendiente de pago				 Cuando se registran los costos de los honorarios al agente aduanal se crea una estimación la cual detona un registro lo mas estimado posible de IVA, el cual será corregido y/o complementado por el pedimento de importación 
        118.02.01	IVA acreditable de importación pagado				 Se registra cuando se tiene el pedimento de importación, compras de productos al extranjero
        -->

        <record id="tax12" model="account.tax.template">
            <field name="chart_template_id" ref="mx_coa_sat"/>
            <field name="name">IVA(0%) COMPRAS</field>
            <field name="description">IVA(0%) COMPRAS</field>
            <field name="amount">0</field>
            <field name="amount_type">percent</field>
            <field name="type_tax_use">purchase</field>
            <field name="account_id" ref="l10n_mx_sat_openbias.account_119_01_01"/>
            <field name="refund_account_id" ref="l10n_mx_sat_openbias.account_119_01_01"/>
            <field name="tag_ids" eval="[(6,0,[ref('tax_tag_0')])]"/>
            <field name="tax_group_id" ref="cfd_mx.tax_group_iva"/>
        </record>
